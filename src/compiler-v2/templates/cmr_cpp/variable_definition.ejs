<%
//generate [1][2]...
function genDelcDimsString(dims)
{
	var ret = "";
	for (var i in dims)
		ret += "[" + dims[i] + "]";
	return ret;
}

function genValueString(dims,values)
{
	if (dims.length == 0)
	{
		//cmrGenEqCCode(out,context,formulas[i]);
		return values[0].toCString();
	} else {
		var ret = "{";
		for (var i in values)
		{
			if (dims.length ==2 && i % dims[0] == 0 && i > 0)
				ret += "},";
			if (dims.length==2 && i % dims[0] == 0 && i < values.length - 1)
				ret += "{";
			ret+=values[i].toCString();
			ret += ",";
		}
		if (dims.length == 2)
			ret += "}";
		ret += "}";
		return ret;
	}
}

function isConst(group)
{
	if (group == "constant")
		return "static const ";
	else
		return "";
}
%>
//Definition of <%- object.latexName %>
<%- isConst(object.group) %><%- object.type %> <%- object.longName %><%- genDelcDimsString(object.dims) %> = <%- genValueString(object.dims,object.formulas) %>;
